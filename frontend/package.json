{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "antd": "^6.2.0",
    "axios": "^1.13.2",
    "framer-motion": "^12.26.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-redux": "^9.2.0",
    "react-scripts": "5.0.1",
    "recharts": "^3.6.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios)/)"
    ],
    "moduleNameMapper": {
      "^antd$": "<rootDir>/src/__mocks__/antd.js",
      "^antd/(.*)$": "<rootDir>/src/__mocks__/antd.js",
      "^@ant-design/icons$": "<rootDir>/src/__mocks__/antdIcons.js",
      "^axios$": "<rootDir>/src/__mocks__/axios.js",
      "^framer-motion$": "<rootDir>/src/__mocks__/framerMotion.js",
      "^recharts$": "<rootDir>/src/__mocks__/recharts.js"
    }
  }
}
